{
    "id": "pexels-plugin",
    "name": "Pexels",
    "version": "1.0.0",
    "manifestVersion": 1,
    "main": "index.html",
    "requirements": {
        "apps": [
            "Spice"
        ]
    },
    "icon": [
        {
            "width": 100,
            "height": 100,
            "path": "icon.png",
            "theme": [
                "all"
            ]
        }
    ],
    "entryPoints": [
        {
            "type": "panel",
            "id": "pexels-plugin.entrypoint1",
            "label": {
                "default": "Pexels Plugin"
            },
            "defaultSize": {
                "width": 300,
                "height": 625
            },
            "sandbox": ["allow-scripts", "allow-same-origin"]
        }
    ]
}
